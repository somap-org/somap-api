{
  "name": "somap-api",
  "version": "1.1.0",
  "description": "A Node.js starter for the Serverless Framework with async/await and unit test support",
  "scripts": {
    "build:dev": "tsc && sls deploy",
    "test": "mocha -r ts-node/register tests/**/*.test.ts",
    "test:content": "mocha -r ts-node/register tests/content/**/*.test.ts",
    "test:live": "mocha -r ts-node/register tests/live/**/*.test.ts",
    "test:userGraph": "mocha -r ts-node/register tests/userGraph/**/*.test.ts"
  },
  "author": "Luca Pirrone",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-nodejs-starter.git"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "serverless-bundle": "^1.4.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@hasezoey/typegoose": "latest",
    "@types/aws-lambda": "^8.10.31",
    "@types/cypress": "^1.1.3",
    "@types/express": "^4.17.6",
    "@types/mongoose": "^5.7.22",
    "@types/node": "^14.0.11",
    "moment": "^2.27.0",
    "mongodb": "^2.2.33",
    "mongoose": "^5.9.15",
    "typescript-express": "0.0.2"
  }
}
